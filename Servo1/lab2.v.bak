module lab2 ();

// Inputs
	input lu; // Lower/Upper case. Define pulse lenght: 0 for lower, 1 for upper

// Asuming clock  c = 50 MHz
// and 256 steps
// t = 1ms, P = t/c = 50'000, sd = t/256
// lenght = sd * number of steps (from initials)
// Where sd is time for one step
	
	parameter [7:0] PWM_length_first_upper = ;
	parameter [7:0] PWM_length_second_upper = ;
	
	parameter [7:0] PWM_length_first_lower = ;
	parameter [7:0] PWM_length_second_lower = ;
	
	reg [7:0] PWM_length_first, PWM_length_second;


endmodule